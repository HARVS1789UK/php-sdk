{
    "name": "citypay/php-sdk",
    "type": "library",
    "description": "CityPay SDK for PHP",
    "keywords": [
        "citypay",
        "payment processing",
        "api"
    ],
    "homepage": "http://citypay.com",
    "license": "MIT",
    "authors": [
        {
            "name": "CityPay and contributors",
            "homepage": "http://citypay.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-curl": "*",
        "ext-json": "*",
        "psr/log": "*",
        "apache/log4php": "2.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": { "CityPay\\" : "src/CityPay" }
    },
    "autoload-dev": {
        "psr-4": { "" : "tests/" }
    },
    "extra": {
       
    }
}
